.bounceIn {
	-webkit-animation: bounceIn 1s .2s ease both;
	-moz-animation: bounceIn 1s .2s ease both;
}

@-webkit-keyframes bounceIn {
	0% {
		opacity: 0;
		-webkit-transform: scale(.3)
	}

	50% {
		opacity: 1;
		-webkit-transform: scale(1.05)
	}

	70% {
		-webkit-transform: scale(.9)
	}

	100% {
		-webkit-transform: scale(1)
	}
}

@-moz-keyframes bounceIn {
	0% {
		opacity: 0;
		-moz-transform: scale(.3)
	}

	50% {
		opacity: 1;
		-moz-transform: scale(1.05)
	}

	70% {
		-moz-transform: scale(.9)
	}

	100% {
		-moz-transform: scale(1)
	}
}

.next-page {
	position: fixed;
	width: 30px;
	height: 30px;
	margin: 0px auto;
	bottom: 10px;
	left: 50%;
	margin-left: -15px;
	z-index: 100;
	background: url(http://activity.dapuimg.com/dapu_page_prev.png) no-repeat 0 0;
	background-size: 100%;
	-webkit-animation: ani_page_next 1.2s ease-in 0s infinite;
	-moz-animation: ani_page_next 1.2s ease-in 0s infinite;
	-ms-animation: ani_page_next 1.2s ease-in 0s infinite;
	animation: ani_page_next 1.2s ease-in 0s infinite;
}

@-webkit-keyframes ani_page_next {
	0% {
		opacity: 0;
		-webkit-transform: translateY(10px);
		transform: translateY(10px)
	}

	40% {
		opacity: 1;
		-webkit-transform: translateY(4px);
		transform: translateY(4px)
	}

	100% {
		opacity: 0;
		-webkit-transform: translateY(-4px);
		transform: translateY(-4px)
	}
}

@-moz-keyframes ani_page_next {
	0% {
		opacity: 0;
		-webkit-transform: translateY(10px);
		transform: translateY(10px)
	}

	40% {
		opacity: 1;
		-webkit-transform: translateY(4px);
		transform: translateY(4px)
	}

	100% {
		opacity: 0;
		-webkit-transform: translateY(-4px);
		transform: translateY(-4px)
	}
}

