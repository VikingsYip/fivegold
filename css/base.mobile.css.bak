/* 轮播样式开始 */
.user-swipe-page {
	text-align: center;
	position: relative;
	overflow: hidden;
	padding: 0px 0px 10px 0px;
}

.user-swipe-page .swipe {
	overflow: hidden;
	visibility: hidden;
	position: relative;
}

.user-swipe-page .swipe-wrap {
	overflow: hidden;
	position: relative;
}

.user-swipe-page .swipe-wrap>div {
	float: left;
	width: 100%;
	position: relative;
}

.user-swipe-page .swipe-wrap .wrap {
	display: block;
	position: relative;
}

.user-swipe-page .swipe-wrap a {
	display: inline-block;
	width: 100%;
	overflow: hidden;
}

.user-swipe-page .swipe-wrap img {
	border: 0px;
	width: 100%;
	vertical-align: middle;
}

.user-swipe-page .swipe-nav {
	position: relative;
	max-width: 680px;
	margin: -36px auto 0;
}

.user-swipe-page .swipe-nav .position {
	text-align: center;
	list-style: none;
	margin: 0;
	padding: 0;
}

.user-swipe-page .swipe-nav .position li {
	display: inline-block;
	width: 5px;
	height: 5px;
	border-radius: 5px;
	background: #141414;
	box-shadow: inset 0 1px 3px black, 0 0 1px 1px #202020;
	margin: 0 2px;
	cursor: pointer;
}

.user-swipe-page .swipe-nav .position li.on {
	box-shadow: inset 0 1px 3px -1px #28b4ea, 0 1px 2px rgba(0, 0, 0, .5);
	background-color: #1293dc;
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #1293dc),
		color-stop(100%, #0f6297) );
	background-image: -webkit-linear-gradient(top, #1293dc, #0f6297);
	background-image: -moz-linear-gradient(top, #1293dc, #0f6297);
	background-image: -ms-linear-gradient(top, #1293dc, #0f6297);
	background-image: -o-linear-gradient(top, #1293dc, #0f6297);
	background-image: linear-gradient(top, #1293dc, #0f6297)
}
/* 轮播样式结束 */
