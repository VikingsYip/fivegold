.dp-menu-box .login-tag{
	display:none;
	position: absolute;
	top:0px;
	right:10px;
	width:10px;
	height:10px;
	border-radius:10px;
	background-color:red;
	animation:myfirst 5s;
	-moz-animation:myfirst 5s; /* Firefox */
	-webkit-animation:myfirst 5s; /* Safari and Chrome */
	-o-animation:myfirst 5s; /* Opera */
	animation-iteration-count: 10;/*定义循环资料，infinite为无限次*/
	-moz--animation-iteration-count: 10;/*定义循环资料，infinite为无限次*/
	-webkit-animation-iteration-count: 10;/*定义循环资料，infinite为无限次*/
	-o-animation-iteration-count: 10;/*定义循环资料，infinite为无限次*/
}

@keyframes myfirst
{
	from {
	width:10px;
	height:10px;
	background:yellow;
	}
	to {
	width:10px;
	height:10px;
	background:red;
	}
}

@-moz-keyframes myfirst /* Firefox */
{
	from {
	width:10px;
	height:10px;
	background:yellow;
	}
	to {
	width:10px;
	height:10px;
	background:red;
	}
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
	from {
	width:10px;
	height:10px;
	background:yellow;
	}
	to {
	width:10px;
	height:10px;
	background:red;
	}
}

@-o-keyframes myfirst /* Opera */
{
	from {
	width:10px;
	height:10px;
	background:yellow;
	}
	to {
	width:10px;
	height:10px;
	background:red;
	}
}

